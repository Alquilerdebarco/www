            <div class="modal-body" id="mailRegister">
                <form id="form-mailRegister">
                    <div class="form-group">
                        <label for="reg_name" class="control-label asterisk"><%= texts.name %></label>
                        <input type="text" id="reg_name" class="form-control required" name="reg_name"
                               placeholder="<%= texts.name %>">
                    </div>
                    <div class="form-group">
                        <label for="reg_lastname" class="control-label asterisk"><%= texts.surname %></label>
                        <input type="text" id="reg_lastname" class="form-control required" name="reg_lastname"
                               placeholder="<%= texts.surname %>">
                    </div>
                    <div class="form-group">
                        <label for="reg_phone" class="control-label asterisk"><%= texts.phone %></label>
                        <input type="text" id="reg_phone" name="reg_phone"
                               class="form-control required phone" minlength="6"
                               placeholder="<%= texts.phone %>">
                    </div>
                    <div class="form-group">
                        <label for="reg_address" class="control-label asterisk"><%= texts.address %></label>
                        <input type="text" id="reg_address" class="form-control required" minlength="4"
                               name="reg_address"
                               placeholder="<%= texts.address %>">
                    </div>
                    <div class="form-group">
                        <label for="reg_email" class="control-label asterisk">Email</label>
                        <input type="email" id="reg_email" class="form-control required" name="reg_email"
                               placeholder="Email">
                    </div>

                    <div class="form-group">
                        <label for="reg_pass" class="control-label asterisk"><%= texts.placeholder_pass %></label>
                        <input type="password" id="reg_pass" class="form-control required" minlength="6"
                               name="reg_pass" placeholder="<%= texts.valid_minlength.replace("[number]", "6") %>">
                    </div>
                    <div class="form-group">
                        <label for="reg_pass_confirm" class="control-label asterisk"><%= texts.repeat_pass %></label>
