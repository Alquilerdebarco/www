<div class="modal fade" id="compose-modal-login" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <span class="inline-block pull-right icon-remove" data-dismiss="modal"></span>
                <div class="text-center"><img width="60px" height="60px" src="/frontEnd/img/img_60_menu_logo.png"
                                              srcset="/frontEnd/img/img_60_menu_logo@2x.png 2x">
                </div>
                <h4 class="modal-title align-center">Alquiler<span>de</span>Barco<span>.com</span></h4>
            </div>
            <form action="/Blogin" method="post" name="formLogin" id="formLogin">
                <div class="modal-body">
                    <div class="col-xs-12">
                        <div class="row">
                            <!--<div class="count-head"><%= texts.login_account %></div>-->
                            <div class="form-group">
                                <input type="email" placeholder="Email" name="email" id="email"
                                       class="form-control required">
                            </div>
                            <div class="form-group">
                                <input type="password" placeholder="<%= texts.placeholder_pass %>" name="password"
                                       id="password"
                                       class="form-control required">
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <button type="submit" class="btn btn-primary" id="sub_button"><%= texts.login %></button>
                </div>

                <div class="modal-footer">
                    <div><a href="/backoffice/login#toreset_pass"><%= texts.forgot_pass %></a></div>
                </div>

            </form>

        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
