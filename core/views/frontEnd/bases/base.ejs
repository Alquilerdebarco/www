                               placeholder="<%= texts.valid_minlength.replace("[number]", "6") %>">
                    </div>
                    <div class="text-center">
                        <button class="btn btn-success register" type="submit"><%= texts.register %></button>
                    </div>
                </form>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
