                    <% }else if(book.ship.conditions.refund == "4"){ %>
                    <p><%= texts.det_bad_day %><%= texts.det_refund5 %></p>
                    <% } %>
                    <p><%- book.ship.conditions.text[0].value %></p>
                    <div class="clearfix"></div>
                    <p><%= texts.det_iva_include %></p>
                </div>

                <div class="clearfix"></div>
                <div class="booking-header col-xs-12">
                    <h3><%= texts.det_localization %></h3>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-12" id="marker">
                    <div style="width: 100%" id="mapGoogle">

                    </div>
                    <br/>

                </div>

                <div class="clearfix"></div>
                <div class="col-xs-12">
                    <% if(cancel) { %>
                    <button class="btn btn-danger btn-cancelb"><%= texts.det_cancel_booking %></button>
                    <% }else{ %>
                    <% if(methodPayment.paypal) { %>
                    <button class="btn btn-paypal"><img class="paypal-img"
                                                        src="/frontEnd/img/img_70_payment_paypal_normal.png"
                                                        srcset="/frontEnd/img/img_70_payment_paypal_normal@2x.png 2x">
                    </button>
                    <% } %>
                    <% if(methodPayment.redsys) { %>
                    <form id="tpvForm2" action="<%= redsys.uri %>"  method="post" class="redsys-form">
                        <table hidden>
                            <tbody>
                            <tr>
                                <td>Ds_SignatureVersion:</td>
                                <td><input name="Ds_SignatureVersion" value="<%= redsys.fparams.Ds_SignatureVersion %>"
                                           required="" size="80"></td>
                            </tr>
                            <tr>
                                <td>Ds_MerchantParameters:</td>
                                <td><input name="Ds_MerchantParameters"
                                           value="<%= redsys.fparams.Ds_MerchantParameters %>"
                                           required="" size="80"></td>
                            </tr>
                            <tr>
                                <td>Ds_Signature:</td>
                                <td><input name="Ds_Signature" value="<%= redsys.fparams.Ds_Signature %>"
                                           required="" size="80"></td>
                            </tr>
                            </tbody>
                        </table>
                        <button type="button" class="btn btn-primary btn-redsys" id="tpvBtn2">
                            <img class="redsys-img"
                                 src="/frontEnd/img/img_70__payment_redsys_normal.png"
                                 srcset="/frontEnd/img/img_70__payment_redsys_normal@2x.png 2x">
                            <% if(book.percentage < 100){ %>
                            <% if(!cancel && first){ %>
                            (<%= texts.det_fpay %>)
                            <% }else if(!cancel && !first){ %>
                            (<%= texts.det_spay %>)
                            <% } %>
                            <% } %>
                        </button>
                    </form>
                    <% } %>
                    <% } %>

                </div>
            </div>
        </div>
    </div>
</div>