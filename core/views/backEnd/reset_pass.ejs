    <title>Acceso | Alquiler de Barco</title>

    <!-- Bootstrap core CSS -->

    <link href="/backend/gentelella/css/bootstrap.min.css" rel="stylesheet">

    <link href="/backend/gentelella/fonts/css/font-awesome.min.css" rel="stylesheet">
    <link href="/backend/gentelella/css/animate.min.css" rel="stylesheet">

    <!-- Custom styling plus plugins -->
    <link href="/backend/gentelella/css/custom.css" rel="stylesheet">
    <link href="/backend/gentelella/css/icheck/flat/green.css" rel="stylesheet">


    <script src="/backend/gentelella/js/jquery.min.js"></script>

    <!--[if lt IE 9]>
    <!--<script src="../assets/js/ie8-responsive-file-warning.js"></script>-->
    <![endif]-->

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <style>
        .banner-login {

            border-bottom: 1px solid #5ccdff;
            height: 140px;
            top: 0;
            position: absolute;
        }

        .image-bg {
            background: url("/common/img/img_backend_login2.jpg") no-repeat;
            min-height: 900px;
            padding: 0;
            margin: 0;
            background-size: cover;
            margin-top: -5px;
        }

        .login-bnt {
            width: 100%;
            color: #fff;
            background-color: #428bca;
            border-color: #357ebd;
        }

        .login-bnt:hover {
            color: #fff;
            background-color: #3276b1;
            border-color: #285e8e;
        }

        .margin-top-logo {
            margin: 0 auto;
            width: 60px;
            padding-top: 40px;
        }

        .iso-logo-text {
            margin: 0 auto;
            width: 205px
        }

        .iso-logo-text a {
            padding-top: 4px;
            color: #fff !important;
        }

        .iso-min-text {
            font-size: 16px !important;
            color: #3276b1;
        }

        .iso-access-form {
            color: #3276b1;
            margin-bottom: 14px;
            font-size: 22px;
        }

        .new-in-site {
            margin-top: 20px;
        }
    </style>

</head>

<body style="background:#F7F7F7;">


<div class="col-xs-12" style="    margin: 0;padding: 0;">
    <div class="col-xs-12 image-bg">
        <div id="wrapper">
            <div id="new_reset_pass" class="animate form">
                <div class="col-xs-12">
                    <div class="margin-top-logo">
                        <img src="/frontEnd/img/img_60_menu_logo.png" srcset="/frontEnd/img/img_60_menu_logo@2x.png 2x"
                             class="logo-header">
                    </div>
                    <div class="iso-logo-text">
                        <a class="navbar-brand inline-block" href="/">Alquiler<span>de</span>Barco<span>.com</span></a>
                    </div>
                </div>
                <div class="clearfix"></div>
                <section class="login_content">
                    <input id="user-id" type="hidden" value="<%= user._id %>">
                    <input id="token" type="hidden" value="<%= user.activationKey %>">

                    <div id="success-reset_pass" style="display: none">
                        <div id="msg-reset-success" style="display: none"
                             class="alert alert-success alert-dismissible fade in" role="alert">
                            <strong>Enhorabuena!</strong> Su contraseña ha sido cambiada.
                        </div>

                        <div id="msg-reset-error" style="display: none"
                             class="alert alert-danger alert-dismissible fade in" role="alert">
                            <strong>Lo sentimos!</strong> Si su problema persiste contacte con el administrador.
                        </div>


                        <div class="separator">

                            <p class="change_link">¿Ya eres miembro?
                                <a href="/backoffice/#tologin" class="to_register"> Entrar </a>
                            </p>

                            <div class="clearfix"></div>
                            <br/>
                        </div>
                    </div>

                    <form id="reset_pass-form" action="/Blogin" method="post" autocomplete="off">
                        <div class="iso-access-form">Cambio de Contraseña</div>

                        <div>
                            <input id="password" type="password" class="form-control" name="password"
                                   placeholder="Contraseña" required=""/>
                        </div>
                        <div id="rp">
                            <input id="password2" type="password" class="form-control" name="repeat-password"
                                   placeholder="Repetir Contraseña" required=""/>
                        </div>
                        <div>
                            <a id="submit-reset_pass" class="btn  btn-default  submit login-bnt">Enviar</a>
                        </div>
                        <div class="clearfix"></div>
                    </form>
                    <!-- form -->
                </section>
                <!-- content -->
            </div>
        </div>
    </div>
</div>
<!-- form validation -->
<script type="text/javascript" src="/backend/gentelella/js/parsley/parsley.min.js"></script>
<!-- input mask -->
<script src="/backend/gentelella/js/input_mask/jquery.inputmask.js"></script>

<script src="/backend/js/reset_pass.js"></script>
</body>

</html>