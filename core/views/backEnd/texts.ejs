                                                <div class="form-group">
                                                    <label class="col-xs-12">
                                                        Descripción (META)
                                                    </label>
                                                    <div class="col-xs-12">
                                                        <textarea required class="form-control"
                                                                  ng-model="frequently_questions.components[3].text[$index].value">
                                                        </textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="ln_solid"></div>
                                    <div class="form-group">
                                        <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3 active">
                                            <button ng-disabled="!validateForm()" ng-click="saveFrequentlyQuestions()"
                                                    type="button" class="btn btn-success"> Guardar
                                            </button>
                                            <button ng-click="" type="button" class="btn btn-primary">
                                                Cancelar
                                            </button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-4 col-xs-12">
            <div class="table-responsive">
                        <table class="table table-striped table-bordered jambo_table bulk_action">
                            <thead>
                                <tr class="headings">
                                    <th class="column-title">texto</th>
                                    <th class="column-title">Menú</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="even" ng-cloak ng-repeat="mt in menuText|orderBy: 'pos'" ng-class-even="$index%2==0" ng-class-odd="$index%2==1">
                                    <td class=" ">{{getNameText(mt.group)}}</td>
                                    <td class=" ">
                                        <div ng-show="mt.pos!=null && mt.pos>=0">
                                            <code>{{mt.pos+1}}</code> 
                                            <i ng-click="menu(mt,'up')"   class="fa fa-long-arrow-up pointer"   ng-show="mt.pos > 0"></i> 
                                            <i ng-click="menu(mt,'down')" class="fa fa-long-arrow-down pointer" ng-show="!mt.last"></i>
                                            <i ng-click="menu(mt,'ban')"  class="fa fa-ban pointer"></i>
                                        </div>
                                        <div ng-show="mt.pos == null">
                                            &nbsp; &nbsp; <i ng-show="!mt.pos" ng-click="menu(mt,'show')" class="fa fa-circle-o pointer"></i>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
        </div>
    </div>
</div>