                        </li>
                    </ul>
                    <div id="myTabContent" class="tab-content">
                        <div role="tabpanel" class="tab-pane active" id="tab_content1" aria-labelledby="info-tab">
                            <br>
                            <div class="clearfix"></div>
                            <div class="col-xs-12">
                                <div class="x_title">
                                    <h2><i class="fa fa-gear"></i> General</h2>
                                    <div class="clearfix"></div>
                                </div>
                                <div class="x_content">
                                    <form class="form-horizontal form-label-left" autocomplete="off">
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Nombre del
                                                Sitio</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input type="text" class="form-control" required ng-model="configurations.general.siteName">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">
                                                Listar Barcos por Relevancia (# Reservas)
                                            </label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <div class="radio col-xs-6">
                                                    <label>
                                                        <input ng-checked="configurations.general.sort"
                                                               ng-click="configurations.general.sort=true"
                                                               type="radio" name="iCheckS"> Si
                                                    </label>
                                                </div>
                                                <div class="radio col-xs-6">
                                                    <label>
                                                        <input ng-checked="!configurations.general.sort"
                                                               ng-click="configurations.general.sort=false"
                                                               type="radio" name="iCheckS"> No
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Sitio
                                                Offline</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <div class=" col-xs-6">
                                                    <label lass="activateUserRegister">
                                                        <input ng-checked="configurations.general.siteOffline"
                                                               ng-click="configurations.general.siteOffline=true"
                                                               type="radio" name="iCheck"> Si
                                                    </label>
                                                </div>
                                                <div class=" col-xs-6">
                                                    <label class="deactivateUserRegister">
                                                        <input ng-checked="!configurations.general.siteOffline"
                                                               ng-click="configurations.general.siteOffline=false"
                                                               type="radio" name="iCheck"> No
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Mensaje
                                                Offline</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <textarea id="message" required class="form-control" ng-model="configurations.general.offlineMessage" name="message"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Dominio</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input type="text" class="form-control" placeholder="http://yourdomain.com" required ng-model="configurations.general.domain">
                                                <h6>http:// ó https://</h6>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Uso Horario</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <select class="form-control" ng-model="configurations.general.timeZone">
                                                    <option ng-repeat="tz in timezones" value="{{tz.text}}">
                                                        {{tz.text}}
                                                    </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                                <button ng-disabled="!validateGeneralData()" ng-click="saveGeneral()" type="button" class="btn btn-success"> Guardar
                                                </button>
                                            </div>
                                        </div>
                                        <div class="ln_solid"></div>
                                    </form>
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="x_title">
                                    <h2><i class="fa fa-code"></i> Meta Data (General)
                                    </h2>
                                    <div class="clearfix"></div>
                                </div>
                                <div class="x_content">
                                    <form class="form-horizontal form-label-left" autocomplete="off">
                                        <div class="form-group">
                                            <div>
                                                <ul id="myTabLaguage" class="nav nav-tabs bar_tabs" role="tablist">
                                                    <li ng-repeat="lang in $root.M_LANGUAGES" role="presentation" ng-class="{'active':$index == selectTab}" ng-click="selectedTab($index)">
                                                        <a id="home-tab" role="tab" data-toggle="tab" aria-expanded="false">{{lang.name}}</a>
                                                    </li>
                                                </ul>
                                                <div id="myTabContentInfo" class="tab-content">
                                                    <div ng-repeat="lang in $root.M_LANGUAGES" role="tabpanel" class="tab-pane fade " ng-class="{active:$index==selectTab,in:$index==selectTab}"
                                                        aria-labelledby="home-tab">
                                                        <div class="clearfix"></div>
                                                        <div class="form-group">
                                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">
                                                                Site Meta Description
                                                            </label>
                                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                                <textarea required class="form-control" ng-model="configurations.metaData.siteMetaDescription[$index].value">
                                                                                </textarea>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Site
                                                                Meta
                                                                Keywords</label>
                                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                                <textarea required class="form-control" ng-model="configurations.metaData.siteMetaKeywords[$index].value">
                                                                                </textarea>
                                                            </div>
                                                        </div>
                                                        <div class="clearfix"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                                <button ng-disabled="!validateMetaDescriptionData()" ng-click="saveMetaDescription()" type="button" class="btn btn-success"> Guardar
                                                </button>
                                            </div>
                                        </div>
                                        <div class="ln_solid"></div>
                                    </form>
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="x_title">
                                    <h2><i class="fa fa-envelope"></i> Email
                                    </h2>
                                    <div class="clearfix"></div>
                                </div>
                                <div class="x_content">
                                    <form class="form-horizontal form-label-left" autocomplete="off">
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Nombre de la
                                                cuenta</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input ng-disabled="!editMail" type="text" class="form-control" required ng-model="configurations.mailSettings.name">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Servidor</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input ng-disabled="!editMail" type="text" class="form-control" required ng-model="configurations.mailSettings.mailServer">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Puerto</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input ng-disabled="!editMail" type="number" class="form-control" required ng-model="configurations.mailSettings.mailPort">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">SSL</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <div id="gender" class="btn-group" data-toggle="buttons">
                                                    <label ng-disabled="!editMail" style="width: 100px" class="btn" ng-class="{'btn-default':!configurations.mailSettings.secureConnection,'btn-primary':configurations.mailSettings.secureConnection}"
                                                        ng-click="configurations.mailSettings.secureConnection = true">
                                                        <input ng-disabled="!editMail"
                                                               ng-model="configurations.mailSettings.secureConnection"
                                                               type="radio" name="mode" value="true"> &nbsp; Si &nbsp;
                                                    </label>
                                                    <label ng-disabled="!editMail" style="width: 100px" class="btn btn-primary" ng-class="{'btn-default':configurations.mailSettings.secureConnection,'btn-primary':!configurations.mailSettings.secureConnection}"
                                                        ng-click="configurations.mailSettings.secureConnection = false">
                                                        <input ng-disabled="!editMail"
                                                               ng-model="configurations.mailSettings.secureConnection"
                                                               type="radio" name="mode" value="false"> No
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Usuario</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input ng-disabled="!editMail" type="text" class="form-control" required ng-model="configurations.mailSettings.user">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Password</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input ng-disabled="!editMail" type="password" class="form-control" placeholder="**************" ng-model="configurations.mailSettings.password">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Dirección de
                                                correo</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input ng-disabled="!editMail" type="text" class="form-control" required ng-model="configurations.mailSettings.email">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                                <button ng-show="editMail" ng-disabled="!validateMailSettingsData()" ng-click="saveMailSettings()" type="button" class="btn btn-success"> Guardar
                                                </button>
                                                <button ng-show="!editMail" ng-click="editMail=true" type="button" class="btn btn-primary">
                                                    Editar
                                                </button>
                                                <button ng-show="!editMail" ng-disabled="!validateMailSettingsData()" class="btn btn-primary" type="button" ng-click="sendTest()">
                                                    Probar
                                                </button>
                                            </div>
                                        </div>
                                        <div class="ln_solid"></div>
                                    </form>
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="x_title">
                                    <h2><i class="fa fa-envelope"></i> Impuestos
                                    </h2>
                                    <div class="clearfix"></div>
                                </div>
                                <div class="x_content">
                                    <form class="form-horizontal form-label-left" autocomplete="off">
                                        <div class="form-group">
                                            <label class="control-label col-md-4 col-sm-4 col-xs-12">Iva%</label>
                                            <div class="col-md-8 col-sm-8 col-xs-12">
                                                <input type="number" class="form-control" ng-model="configurations.iva">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                                <button ng-click="saveIva()" type="button" ng-disabled="validateIva()" class="btn btn-success"> Guardar
                                                </button>
                                            </div>
                                        </div>
                                        <div class="ln_solid"></div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <div role="tabpanel" class="tab-pane fade" id="tab_content2" aria-labelledby="boats-tab">
                            <br>
                            <div class="clearfix"></div>
                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <div ng-hide="showFormExperience">
                                        <div class="x_title">
                                            <h2><i class="fa fa-certificate"></i> Experiencias disponibles</h2>
                                            <button class="btn btn-success btn-xs pull-right" ng-click="changeShowFormExperience(true)">
                                                Crear Experiencia
                                            </button>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="x_content">
                                            <div class="table-responsive">

                                                <table class="table table-condensed table-striped table-bordered jambo_table bulk_action">
                                                    <thead>
                                                        <tr class="headings">
                                                            <th class="column-title">Nombre</th>
                                                            <th style="min-width:126px" class="column-title">Acciones</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="experience in configurations.shipSettings.experiences" ng-show="!experience.remove" ng-class-even="$index%2==0"
                                                            ng-class-odd="$index%2==1">
                                                            <td>{{experience.name[0].value}}</td>
                                                            <td class="td-button-padding td-button-padding">
                                                                <button ng-show="experience.default" ng-click="unCheckExperience(experience._id)" class="btn btn-xs btn-success">
                                                                <i class="fa fa-check"></i>
                                                            </button>
                                                                <button ng-show="!experience.default" ng-click="defaultExperience(experience._id)" class="btn btn-warning btn-xs">
                                                                <i class="fa fa-ban"></i>
                                                            </button>
                                                                <button class="btn btn-info btn-xs " ng-click="selExperience(experience)">
                                                                <i class="fa fa-edit"></i>
                                                            </button>
                                                                <button class="btn btn-danger btn-xs " ng-click="delExperience(experience)">
                                                                <i class="fa fa-trash"></i>
                                                            </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-show="showFormExperience">
                                        <div class="x_title">
                                            <h2>Crear Nueva Experiencia</h2>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="x_content">
                                            <form class="form-horizontal form-label-left" autocomplete="off">
                                                <div>
                                                    <ul id="myTabLaguage" class="nav nav-tabs bar_tabs" role="tablist">
                                                        <li ng-repeat="lang in $root.M_LANGUAGES" role="presentation" ng-class="{'active':$index == selectTab}" ng-click="selectedTab($index)">
                                                            <a id="home-tab" role="tab" data-toggle="tab" aria-expanded="false">{{lang.name}}</a>
                                                        </li>
                                                    </ul>
                                                    <div id="myTabContentInfo" class="tab-content">
                                                        <div ng-repeat="lang in $root.M_LANGUAGES" role="tabpanel" class="tab-pane fade " ng-class="{active:$index==selectTab,in:$index==selectTab}"
                                                            aria-labelledby="home-tab">
                                                            <br>
                                                            <div class="clearfix"></div>
                                                            <div class="form-group">
                                                                <label class="col-xs-12">
                                                                    Nombre del tipo de Experiencia
                                                                </label>
                                                                <div class="col-xs-12">
                                                                    <input required class="form-control" ng-model="experience.name[$index].value" />
                                                                </div>
                                                            </div>
                                                            <!--<div class="form-group">-->
                                                            <!--<label class="col-xs-12">-->
                                                            <!--Descripción del tipo de Experiencia-->
                                                            <!--</label>-->
                                                            <!--<div class="col-xs-12">-->
                                                            <!--<textarea required class="form-control"-->
                                                            <!--ng-model="experience.description[$index].value"></textarea>-->
                                                            <!--</div>-->
                                                            <!--</div>-->
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="ln_solid"></div>
                                                <div class="form-group">
                                                    <div class="col-xs-12 col-md-offset-3">
                                                        <button ng-show="!editExperience" ng-disabled="!validateAddExperience()" ng-click="addExperience()" type="button" class="btn btn-success">Crear
                                                        </button>
                                                        <button ng-show="editExperience" ng-disabled="!validateAddExperience()" ng-click="saveExperience()" type="button" class="btn btn-success"> Guardar
                                                        </button>
                                                        <button ng-click="changeShowFormExperience(false)" type="button" class="btn btn-primary">
                                                            Cancelar
                                                        </button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <div ng-hide="showFormType">
                                        <div class="x_title">
                                            <h2><i class="fa fa-anchor"></i> Barcos disponibles</h2>
                                            <button class="btn btn-success btn-xs pull-right" ng-click="showAddType()">
                                                Crear Tipo de Barco
                                            </button>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="x_content">
                                            <div class="table-responsive">
                                                <table class="table table-condensed table-striped table-bordered jambo_table bulk_action">
                                                    <thead>
                                                        <tr class="headings">
                                                            <th class="column-title">Nombre</th>
                                                            <th style="min-width:86px" class="column-title">Acciones</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="typ in types" ng-class-even="$index%2==0" ng-class-odd="$index%2==1">
                                                            <td>{{typ.name[0].value}}</td>
                                                            <td class="td-button-padding">
                                                                <button ng-click="showType(typ)" class="btn btn-xs btn-primary">
                                                                <i class="fa fa-edit"></i>
                                                            </button>
                                                                <button class="btn btn-danger btn-xs" data-toggle="modal" data-target=".delete-type" ng-click="selectType(typ)">
                                                                <i class="fa fa-trash"></i>
                                                            </button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-show="showFormType">
                                        <div class="x_title">
                                            <h2>Crear Nuevo Tipo de Barco</h2>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="x_content">
                                            <form class="form-horizontal form-label-left" autocomplete="off">
                                                <div class="form-group">
                                                    <div>
                                                        <ul id="myTabLaguage" class="nav nav-tabs bar_tabs" role="tablist">
                                                            <li ng-repeat="lang in $root.M_LANGUAGES" role="presentation" ng-class="{'active':$index == selectTab}" ng-click="selectedTab($index)">
                                                                <a id="home-tab" role="tab" data-toggle="tab" aria-expanded="false">{{lang.name}}</a>
                                                            </li>
                                                        </ul>
                                                        <div id="myTabContentInfo" class="tab-content">
                                                            <div ng-repeat="lang in $root.M_LANGUAGES" role="tabpanel" class="tab-pane fade " ng-class="{active:$index==selectTab,in:$index==selectTab}"
                                                                aria-labelledby="home-tab">
                                                                <br>
                                                                <div class="clearfix"></div>
                                                                <div class="form-group">
                                                                    <label class="col-xs-12">
                                                                        Nombre del tipo de Barco
                                                                    </label>
                                                                    <div class="col-xs-12">
                                                                        <input required class="form-control" ng-model="type.name[$index].value" ng-disabled="disabledFormType" />
                                                                    </div>
                                                                </div>
                                                                <!--<div class="form-group">-->
                                                                <!--<label class="col-xs-12">-->
                                                                <!--Descripción del tipo de Barco-->
                                                                <!--</label>-->
                                                                <!--<div class="col-xs-12">-->
                                                                <!--<textarea required class="form-control"-->
                                                                <!--ng-disabled="disabledFormType"-->
                                                                <!--ng-model="type.description[$index].value"></textarea>-->
                                                                <!--</div>-->
                                                                <!--</div>-->
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="ln_solid"></div>
                                                <div class="form-group">
                                                    <div class="col-xs-12 col-md-offset-3">
                                                        <button ng-show="addForm" ng-disabled="!validateTypes()" ng-click="addTypes()" type="button" class="btn btn-success">Crear
                                                        </button>
                                                        <button ng-show="!addForm" ng-disabled="!validateTypes()" ng-click="saveTypes()" type="button" class="btn btn-success">Guardar
                                                        </button>
                                                        <button ng-click="initializeType()" type="button" class="btn btn-primary">
                                                            Cancelar
                                                        </button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <div ng-hide="showAddEquipment">
                                        <div class="x_title">
                                            <h2><i class="fa fa-suitcase"></i> Equipamiento</h2>
                                            <button ng-show="!showEdit" class="btn btn-success btn-xs pull-right" ng-click="selectAddEquipment()">
                                                Crear Equipamiento
                                            </button>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="x_content">
                                            <div class="table-responsive">

                                                <table class="table table-striped table-bordered jambo_table bulk_action table-condensed">
                                                    <thead>
                                                        <tr class="headings">
                                                            <th class="column-title">Nombre</th>
                                                            <th class="column-title"># Equipos</th>
                                                            <th style="min-width:86px" class="column-title">Acciones</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="eq in equipments" class="pointer" ng-click="selectEquipment(eq)" ng-class="{'odd':($index%2)==0,'even':($index%2)!=0,'selected':equipment._id==eq._id}">
                                                            <td>{{eq.name[0].value}}</td>
